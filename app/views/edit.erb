<form method="POST" action="/posts/<%=@post.id%>">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <label>Blog Name: <input type="text" value="<%=@post.name%>" name="name"/></label>
  <label>Blog Content: <input type="text" value="<%=@post.content%>" name="content"/></label>
  <input type="submit" value="submit">
</form>
